{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-eslint-rules", "tslint-consistent-codestyle", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "severity": "warning",
      "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
    }
  },
  "rulesDirectory": []
}
